
@{
    ViewBag.Title = "CreateAbout";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Hakkımda Düzenleme sayfası</h2>

<form method="post">

    <label>Resim URL'si</label>
    <input type="text" class="form-control" name="ImageURL" />

    <label>Başlık</label>
    <input type="text" class="form-control" name="Title" />

    <label>Doğum Tarihi</label>
    <input type="datetime" class="form-control" name="BirthDay" />

    <label>Web Sitesi</label>
    <input type="text" class="form-control" name="WebSite" />

    <label>Telefon</label>
    <input type="text" class="form-control" name="Phone" />

    <label>Şehir</label>
    <input type="text" class="form-control" name="City" />

    <label>Yaş</label>
    <input type="number" class="form-control" name="Age" />

    <label>E posta</label>
    <input type="text" class="form-control" name="Email" />

    <label>FreeLance</label>
    <input type="text" class="form-control" name="FreeLance" />

    <label>Açıklama1</label>
    <input type="text" class="form-control" name="Description1" />

    <label>Açıklama2</label>
    <input type="text" class="form-control" name="Description2" />

    <label>Eğitim Seviyesi</label>
    <input type="text" class="form-control" name="Degree" />

    <button class="btn btn-success">Ekle</button>

</form>