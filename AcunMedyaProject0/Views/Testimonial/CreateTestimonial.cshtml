
@{
    ViewBag.Title = "CreateTestimonial";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Referans Oluşturma Sayfası</h2>

<form method="post">

    <label>Açıklama1</label>
    <input type="text" class=" form-control" name="Description1" />

    <label>Referans Adı</label>
    <input type="text" class=" form-control" name="TestimonialName" />

    <label>Açıklama2</label>
    <input type="text" class=" form-control" name="Description2" />

    <label>Resim Linki</label>
    <input type="text" class=" form-control" name="ImageUrl" />

    <label>Başlık</label>
    <input type="text" class=" form-control" name="Title" />

    <button class="btn btn-success">Ekle</button>
</form>

